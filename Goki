#include<iostream>
using namespace std;

int main()
{
	//Declarations
	int N, X;
	
	
	//Statements
	
	cin >> N;
	
	cin >> X;
	
	int A[N];
	
  
	for(int i=0; i < N; i++)
	{
		cin >> A[i];
	}

    //conditions
	for(int j=0; j < N; j++)
	{
		if(A[j] >= X)
		cout << A[j] <<"\tYES\n";
		else
		cout << A[j] <<"\tNO\n";
	}

	return 0;
}
