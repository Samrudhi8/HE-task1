//A program which takes an integer input and prints the sum of the digits
//Written By : B420045 Samrudhi Panda
#include<iostream>

using namespace std;

int main()
{
	int num,res = 0;
	cout << "Enter a number to find the sum of the digits ";
	cin >> num;
	
	res = num % 10;
	num = num/10;
	res = res + num;
	
	cout << "The sum of digits is:" << res;
	
	return 0;
	}
