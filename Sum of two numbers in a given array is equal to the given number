#include<iostream>
#include<bits/stdc++.h>

using namespace std;

bool solve(int A[],int arr_size,int num);

int main()
{
    int nums[] = {35,8,18,3,22};
    int k = 11;
    int arr_size = sizeof(nums);

    if (solve(nums,arr_size,k))
    cout << "True";
    else
    cout << "False";
    
    return 0;

}
bool solve(int A[],int arr_size,int num) 
{ 
    int i = 0,j;
    
    sort(A,A + arr_size);
    
    j = arr_size - 1;

    while(i < j)
    {
        if (A[i] + A[j] == num)
        return 1;
        else if (A[i] + A[j] < num)
         i++;
        else
         j--;
    }
    return 0;
}
