#include<iostream>
using namespace std;

int main()
{
	int T;
	int M,N;
	
	cout << "Enter the number of testcases\n";
	cin >> T;
	
	cout << "Enter number of friends and number of chocolates in each test case:\n";
	
	for(int i = 0; i < T; i++)
	{
		cin >> M >> N;
		
		if (M % N == 0)
		cout << "Yes\n";
		else
		cout << "No\n";
	}
	
	return 0;
	
}
